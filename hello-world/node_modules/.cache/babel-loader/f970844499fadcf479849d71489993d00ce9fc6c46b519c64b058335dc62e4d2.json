{"ast":null,"code":"import TaskInput from \"./components/TaskInput.vue\";\nimport TaskCard from \"./components/TaskCard.vue\";\nimport { ref } from 'vue';\nexport default {\n  name: 'App',\n  components: {\n    TaskCard,\n    TaskInput\n  },\n  setup() {\n    const taskList = ref([{\n      id: 0,\n      title: 'Create video',\n      description: 'And upload on YT',\n      status: false\n    }]);\n    const addTask = ({\n      title,\n      description\n    }) => {\n      taskList.value = [...taskList.value, {\n        id: taskList.value[taskList.value.length - 1].id + 1,\n        title,\n        description,\n        status: false\n      }];\n    };\n    const setDoneTask = id => {\n      taskList.value = taskList.value.map(x => {\n        if (x.id === id) x.status = true;\n        return x;\n      });\n    };\n    const removeTask = id => {\n      taskList.value = taskList.value.filter(x => x.id !== id);\n    };\n    return {\n      taskList,\n      addTask,\n      removeTask,\n      setDoneTask\n    };\n  }\n};","map":{"version":3,"names":["TaskInput","TaskCard","ref","name","components","setup","taskList","id","title","description","status","addTask","value","length","setDoneTask","map","x","removeTask","filter"],"sources":["C:\\Users\\foxyp\\OneDrive\\Документы\\GitHub\\sait\\hello-world\\src\\App.vue"],"sourcesContent":["<template>\n    <nav class=\"navbar\">\n        <div class=\"container_nav\">\n            <a href=\"#\" class=\"navbar-brand\">\n                <img src=\"img/logo.png\" alt=\"\">\n            </a>\n            <div class=\"navbar-wrap\">\n                <ul class=\"navbar-menu\">\n                    <li><a href=\"#\">Проекты</a></li>\n                </ul>\n            </div>\n\n        </div>\n    </nav>\n    <main>\n      <div class=\"\">\n        <span class=\"project_name\">RimWorld</span>\n      </div>\n        <div class=\"container\">\n            <div class=\"container_menu\">\n                <!-- <div class=\"menu\">\n                    <span>Другие доски</span>\n                </div> -->\n              <div class=\"block\">\n                <div class=\"block_name\">\n                  <span>Игровой процесс</span>\n                  <p>Что надо сделать</p>\n                </div>\n                <TaskInput @onAddTask=\"addTask\"></TaskInput>\n                <ul class=\"task-list my-list\">\n                  <li v-for=\"item in taskList\" :key=\"item.id\">\n                    <TaskCard @onRemove=\"removeTask(item.id)\" @onDone=\"setDoneTask(item.id)\" :model=\"item\"></TaskCard>\n                  </li>\n                </ul>\n              </div>\n            </div>\n        </div>\n      </main>\n</template>\n\n<script>\nimport TaskInput from \"./components/TaskInput.vue\";\nimport TaskCard from \"./components/TaskCard.vue\";\nimport {ref} from 'vue'\n\nexport default {\n  name: 'App',\n  components: {\n    TaskCard,\n    TaskInput,\n  },\n  setup() {\n    const taskList = ref([{id: 0, title: 'Create video', description: 'And upload on YT', status: false}])\n\n    const addTask = ({title, description}) => {\n      taskList.value = [...taskList.value, {id: taskList.value[taskList.value.length - 1].id + 1, title, description, status: false}]\n    }\n\n    const setDoneTask = (id) => {\n      taskList.value = taskList.value.map(x => {\n        if(x.id === id)\n          x.status = true\n        return x\n      })\n    }\n\n    const removeTask = (id) => {\n      taskList.value = taskList.value.filter(x => x.id !== id)\n    }\n\n    return {\n      taskList,\n      addTask,\n      removeTask,\n      setDoneTask\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .task-list {\n    list-style: none;\n  }\n</style>"],"mappings":"AAyCA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAAQC,GAAG,QAAO,KAAI;AAEtB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,QAAQ;IACRD;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIJ,GAAG,CAAC,CAAC;MAACK,EAAE,EAAE,CAAC;MAAEC,KAAK,EAAE,cAAc;MAAEC,WAA